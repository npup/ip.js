{
  "name": "ip-js",
  "description": "number interpolation utility",
  "version": "0.10.0",
  "private": true,
  "main": "./build/ip.js",
  "devDependencies": {
    "buster": "^0.7.13",
    "eslint": "^0.6.2",
    "uglify-js": "^2.4.14"
  },
  "scripts": {
    "lint": "npm run build && eslint --config .eslintrc ./build/ip.dev.js || `exit 0`",
    "test": "npm run build && node ./test/ip-test.js",
    "build": "rm -rf ./build/* && cat src/ip.js > ./build/ip.dev.js && uglifyjs ./build/ip.dev.js > ./build/ip.js"
  }
}
